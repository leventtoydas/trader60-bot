# Rename/edit this file or set these as Railway Environment Variables
TELEGRAM_TOKEN=YOUR_TELEGRAM_BOT_TOKEN
TELEGRAM_CHAT_ID=YOUR_TELEGRAM_CHAT_ID

# Optional provider keys (not required when using yfinance)
TWELVEDATA_KEY=
FINNHUB_KEY=

# Comma-separated symbols list (leave empty to use defaults)
SYMBOLS=

# Timeframes (comma separated): 5m,15m,30m,1h,4h
TIMEFRAMES=5m,15m,30m,1h,4h

# Minutes to suppress duplicate signals for same (symbol, tf, side)
DEBOUNCE_MIN=60

# Timezone for scheduling
APP_TIMEZONE=Europe/Istanbul
